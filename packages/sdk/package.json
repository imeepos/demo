{
  "name": "@sker/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for 舆情分析系统 API",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "openapi-ts --file openapi-ts.config.ts",
    "dev": "tsc --watch",
    "clean": "rimraf dist src/generated"
  },
  "keywords": [
    "api",
    "sdk",
    "typescript",
    "sentiment-analysis",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.84.2",
    "typescript": "^5.7.3",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  }
}
